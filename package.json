    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.6.0",
    "jasmine-reporters": "^2.2.0",
    "jest": "^23.6.0",
    "jsdom": "^9.2.1",
    "lint-staged": "^3.2.6",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
